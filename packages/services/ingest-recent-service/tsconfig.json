{
	"extends": "../../../tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"module": "ESNext",
		"target": "ES2020",
		"lib": ["ES2020"]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist", "**/*.test.ts"]
}
